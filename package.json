{
  "name": "@tmikeschu/graphql-typescript-response-factories",
  "version": "1.0.2",
  "main": "./build/index.js",
  "types": "./build/",
  "scripts": {
    "build": "rm -rf build; ./node_modules/.bin/tsc",
    "prepare": "npm run build",
    "format": "prettier --write 'src/**/*.{ts,js,tsx,jsx}'",
    "graphql-codegen": "graphql-codegen --config ./integration/graphql-codegen.yml",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@graphql-codegen/cli": "^1.13.1",
    "@graphql-codegen/plugin-helpers": "^1.13.1",
    "graphql": "^15.3.0",
    "react": "^16.13.1",
    "ts-poet": "^3.2.2"
  },
  "devDependencies": {
    "@apollo/client": "^3.1.4",
    "@graphql-codegen/typescript-operations": "^1.12.1",
    "@graphql-codegen/typescript-react-apollo": "^2.0.6",
    "@tmikeschu/graphql-typescript-factories": "^1.0.2",
    "@types/jest": "^26.0.7",
    "@types/react": "^16.9.43",
    "eslint-config-prettier": "^6.5.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.4",
    "typescript": "^4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run format"
    }
  }
}
